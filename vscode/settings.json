{
  "editor.fontFamily": "FiraCode NF Retina",
  "editor.fontLigatures": true,
  "workbench.colorTheme": "Tokyo Night",
  "editor.rulers": [80, 120],
  "workbench.startupEditor": "none",
  // Scroll only along the predominant axis when scrolling both vertically and horizontally at the same time.
  // Prevents horizontal drift when scrolling vertically on a trackpad.
  "editor.scrollPredominantAxis": true,
  // Controls the minimal number of visible leading and trailing lines surrounding the cursor.
  // Known as 'scrollOff' or 'scrollOffset' in some other editors.
  "editor.cursorSurroundingLines": 5,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    }
  },
  "terminal.integrated.cursorBlinking": true,
  "json.schemas": [],
  "workbench.iconTheme": "material-icon-theme",
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "snyk.yesWelcomeNotification": false,
  "editor.accessibilitySupport": "off",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1,
    "esbenp.prettier-vscode": 2,
    "vscode.typescript-language-features": 3,
    "Angular.ng-template": 4,
    "eamodio.gitlens": 5,
    "waderyan.gitblame": 5,
    "vscode.git": 5,
    "dbaeumer.vscode-eslint": 6
  },
  "vscode-neovim.useWSL": true,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
  "vscode-neovim.logOutputToConsole": true,
  "editor.lineNumbers": "relative",
  "editor.stickyScroll.enabled": true,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.codeLens.scopes": ["document", "containers"],
  "gitlens.statusBar.enabled": false,
  "cSpell.userWords": [
    "amrock",
    "chiplist",
    "DOCUMENTTYPE",
    "dtsx",
    "Ebinder",
    "esbenp",
    "FIPS",
    "mzawisa",
    "neovim",
    "netcoreapp",
    "netstandard",
    "NOSONAR",
    "nrwl",
    "nvim",
    "oidc",
    "overflowable",
    "snyk",
    "sonarlint",
    "sonarqube",
    "Sysnative",
    "trackpad",
    "windir"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
  "workbench.preferredHighContrastColorTheme": "One Dark Pro Darker",
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "material-icon-theme.hidesExplorerArrows": true,
  "editor.renderWhitespace": "trailing",
  "editor.guides.bracketPairs": "active",
  "editor.linkedEditing": true,
  "editor.showFoldingControls": "never",
  "[scss]": {
    "editor.tabSize": 4,
    "editor.indentSize": "tabSize"
  },
  "editor.detectIndentation": true,
  "[html]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.wrapLineLength": 120,
  "editor.wordWrapColumn": 120,
  "git.openRepositoryInParentFolders": "never",
  "eslint.format.enable": true,
  "prettier.requireConfig": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "typescript.disableAutomaticTypeAcquisition": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "window.zoomLevel": 1,
  "json.schemaDownload.enable": true,
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "hint"],
  "security.allowedUNCHosts": ["mi"],
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "editor.formatOnPaste": true,
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  }
}
